# -*- coding: utf-8 -*-
import dataiku
import pandas as pd

# Read recipe inputs
DP67_cpp_rev01 = dataiku.Dataset("DP67_cpp_rev01")
DP67_cpp_rev01_df = DP67_cpp_rev01.get_dataframe()

# Compute recipe outputs from inputs
# 첫 번째 행은 남기고, 2, 3, 4행을 제외한 뒤 5행부터 남김
DP67_cpp_df_df = pd.concat([DP67_cpp_rev01_df.iloc[[0]], DP67_cpp_rev01_df.iloc[4:]], ignore_index=True)

# Write recipe outputs
DP67_cpp_df = dataiku.Dataset("DP67_cpp_df")
DP67_cpp_df.write_with_schema(DP67_cpp_df_df)

